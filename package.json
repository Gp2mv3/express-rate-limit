{
	"name": "express-rate-limit",
	"version": "0.0.0-typescript-beta-7",
	"description": "Basic IP rate-limiting middleware for Express. Use to limit repeated requests to public APIs and/or endpoints such as password reset.",
	"author": {
		"name": "Nathan Friedly",
		"url": "http://nfriedly.com/"
	},
	"license": "MIT",
	"homepage": "https://github.com/nfriedly/express-rate-limit",
	"repository": "nfriedly/express-rate-limit",
	"keywords": [
		"express-rate-limit",
		"express",
		"rate",
		"limit",
		"ratelimit",
		"rate-limit",
		"middleware",
		"ip",
		"auth",
		"authorization",
		"security",
		"brute",
		"force",
		"bruteforce",
		"brute-force",
		"attack"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "tsc",
		"pretest": "npm run build",
		"lint": "eslint .",
		"autofix": "npm run lint -- --fix",
		"test": "npm run lint && cross-env TS_NODE_PROJECT=test/tsconfig.json mocha --require ts-node/register test/*-test.*",
		"precommit": "pretty-quick --staged",
		"prepublish": "npm run build"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/mocha": "^7.0.2",
		"@types/node": "^12.0.12",
		"@types/supertest": "^2.0.9",
		"@typescript-eslint/parser": "^3.4.0",
		"cross-env": "^7.0.2",
		"eslint": "^7.3.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"express": "^4.17.1",
		"husky": "^4.2.5",
		"mocha": "^8.0.1",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"supertest": "^4.0.2",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.5"
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"singleQuote": true,
		"bracketSpacing": true,
		"trailingComma": "all",
		"proseWrap": "always"
	}
